IN 10H
MVI B,01H 		
MVI C,F4H 		
MVI D,01H 		
			
MVI E,01H		
			

MAIN:
LDA 2000H
ANI 02H 		
JZ SKIP		
MVI A,01H 		
CALL TADA
JMP MAIN		

SKIP:
MOV A,D
CALL TADA
LDA 2000H
ANI 01H 		
JNZ WEIRDSTUFF	

ROUND_LOOP: 		
MOV A,D
RLC
MOV D,A
JMP MAIN		

WEIRDSTUFF:
MOV A,E 
ANI 01H		
JNZ WL			

WR: 	
MOV A,D
RRC
MOV D,A
MOV A,D
ANI 01H 		 
JZ MAIN		 
MVI E,01H		 
JMP MAIN

WL: 		
MOV A,D
RLC
MOV D,A
MOV A,D
ANI 80H	
JZ MAIN		 
MVI E,00H
JMP MAIN

TADA:		
CMA		 
STA 3000H
CALL DELB ;replaced with 3 "NOP"s when run step by step
CMA
RET

END